<project name="XXX" default="build" basedir=".">

	<!-- $Id$ -->

	<property name="src" value="."/>
	<property name="build" value="build"/>
	<property name="darwinsys.jar"
		value="${user.home}/classes/com-darwinsys-all.jar"/>

	<target name="init">
